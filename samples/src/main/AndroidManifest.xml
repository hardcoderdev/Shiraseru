<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hardcoder.dev.shiraseru_samples">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Toaster"
        tools:targetApi="31">
        <activity
            android:name=".snacker.core.SnackerCoreActivity"
            android:exported="false" />
        <activity
            android:name=".snacker.core.SnackerCoreDSLActivity"
            android:exported="false" />
        <activity
            android:name=".snacker.builtIn.SnackerBuiltInActivity"
            android:exported="false" />
        <activity
            android:name=".snacker.builtIn.SnackerBuiltInDSLActivity"
            android:exported="false" />
        <activity
            android:name=".toaster.core.ToasterCoreActivity"
            android:exported="false" />
        <activity
            android:name=".toaster.core.ToasterCoreDSLActivity"
            android:exported="false" />
        <activity
            android:name=".toaster.builtIn.ToasterBuiltInActivity"
            android:exported="false" />
        <activity
            android:name=".toaster.builtIn.ToasterBuiltInDSLActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>